using System; 

class Program {

    static void Main (string [] args) { 

   int A = int.Parse (Console.ReadLine ());
   int B = int.Parse (Console.ReadLine ());
   int Soma = A + B;
   Console.WriteLine ($ "SOMA = {Soma}");

  }
}
