using System; 

class Program {

    static void Main (string [] args) { 

   double raio = double.Parse(Console.ReadLine());
   double raioAoCubo = raio * raio * raio;
   double volume = ((4f/3f) * 3.14159) * raioAoCubo;
   Console.WriteLine($"VOLUME = {volume:0.000}");   

  }
}
